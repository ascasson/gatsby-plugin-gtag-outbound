{
  "name": "gatsby-plugin-gtag-outbound",
  "version": "1.0.3",
  "description": "Custom gatsby plugin to add Google Analytics gtag.js to a site, including Outbound Links.",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir .",
    "precommit": "lint-staged",
    "publish": "npm run build"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "google-analytics",
    "gtag"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ascasson/gatsby-plugin-gtag-casson"
  },
  "author": "Andrey Salomatin <andrey@flpvsk.com>",
  "contributors": [
    {
      "name": "Andrey Salomatin",
      "email": "andrey@flpvsk.com"
    },
    {
      "name": "Anthony Casson",
      "email": "anthony.casson@gmail.com"
    }
  ],
  "license": "MIT",
  "peerDependencies": {
    "gatsby": ">2.0.0-alpha"
  },
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-core": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-react": "^6.0.0",
    "babel-preset-stage-3": "^6.24.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "prettier": "^1.12.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --config .prettierrc.js --write",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "babel-preset-stage-3",
      "babel-preset-es2015",
      "babel-preset-react"
    ]
  }
}
